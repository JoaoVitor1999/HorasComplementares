@model ProjetoHorasCompl.Models.TipoComprovante

@{
    ViewData["Title"] = "Delete";
}

<h2>Deletar</h2>

<h3>Você realmente deseja deletar este tipo de comprovante?</h3>
<div>
    <h4>Tipo Comprovante</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tipo Comprovante
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TipNome)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Deletar" class="btn btn-default" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
