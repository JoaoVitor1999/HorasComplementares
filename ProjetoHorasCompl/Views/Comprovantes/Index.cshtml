@model IEnumerable<ProjetoHorasCompl.Models.Comprovante>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CmpDescricao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CmpDataInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CmpDataFim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CmpQtdHoras)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CmpDescricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CmpDataInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CmpDataFim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CmpQtdHoras)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
